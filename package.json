{
  "name": "twofast-twomcp",
  "private": true,
  "scripts": {
    "setup": "npm install --prefix backend && npm install --prefix frontend",
    "dev": "concurrently -k \"node backend/index.js\" \"npm --prefix frontend run dev\"",
    "dev:quick": "npm run setup && npm run dev",
    "dev:backend": "node backend/index.js",
    "dev:frontend": "npm --prefix frontend run dev"
  },
  "devDependencies": {
    "concurrently": "^9.0.0"
  }
}
