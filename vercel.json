{
  "version": 2,
  "builds": [
    {
      "src": "saa-s-app-skeleton/package.json",
      "use": "@vercel/next"
    }
  ],
  "installCommand": "cd saa-s-app-skeleton && pnpm install --no-frozen-lockfile",
  "buildCommand": "cd saa-s-app-skeleton && pnpm run build",
  "outputDirectory": "saa-s-app-skeleton/.next"
}
